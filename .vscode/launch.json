   "version": "0.2.0",
   "configurations": [
      {
         "name": "Get version help",
         "type": "python",
         "request": "launch",
         "program": "${workspaceFolder}/version_manager.py",
         "console": "integratedTerminal",
         "args": [
            "version",
            "get",
            "help"
         ]
      },
      {
         "name": "Get email help",
         "type": "python",
         "request": "launch",
         "program": "${workspaceFolder}/version_manager.py",
         "console": "integratedTerminal",
         "args": [
            "email",
            "help"
         ]
      },
      {
         "name": "Get generate help",
         "type": "python",
         "request": "launch",
         "program": "${workspaceFolder}/version_manager.py",
         "console": "integratedTerminal",
         "args": [
            "generate",
            "help"
         ]
      },
      {
         "name": "Version get diff",
         "type": "python",
         "request": "launch",
         "program": "${workspaceFolder}/version_manager.py",
         "console": "integratedTerminal",
         "args": [
            "version",
            "get",
            "diff",
            "HEAD",
            "HEAD~1"
         ]
      }
   ]
}
